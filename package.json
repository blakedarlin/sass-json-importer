        "@tsconfig/node20": "^20.1.4",
        "@types/eslint": "^9",
        "@types/eslint__js": "^8.42.3",
        "@types/jest": "^29.5.12",
        "@types/node": "^22.5.2",
        "eslint": "^9.9.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jest": "^28.8.0",
        "eslint-plugin-unicorn": "^55.0.0",
        "globals": "^15.9.0",
        "jest": "^29.7.0",
        "prettier": "^3.3.3",
        "sass-embedded": "^1.78.0",
        "ts-jest": "^29.2.5",
        "ts-jest-resolver": "^2.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.2.0"
